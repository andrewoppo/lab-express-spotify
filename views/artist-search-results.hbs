<div class="artist-list">
    {{#each artistInfo}}
        <div class="artist">
            <img src="{{images.0.url}}" alt="{{name}} image">
            <div class="name-box">
                <p>{{name}}</p>
                <a href="/albums/{{id}}" class="button">View Albums</a>
            </div>
        </div>
    {{/each}}
</div>